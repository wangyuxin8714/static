<stage name="HeySlick" 
	friendlyName="Hey Slick"
	stageWidth="800" 
	stageHeight="600"
	time="0:45" >
	<environment bgLayout="resources/backdrops/LostCity/bgLayout.xml" 
				 fgLayout="resources/backdrops/LostCity/fgLayout.xml"
				 mgLayout="resources/backdrops/LostCity/mgLayout.xml" />
				 
	<playerFish config="config/Fish/JDPlayer.xml" >
		<size size="9.5" targetScore="0" scale="0.38" />
		<size size="14.5" targetScore="600" scale="0.635" />
		<size size="17.5" targetScore="1400" scale="1.0" />
		<size size="18.5" targetScore="2100" scale="1.0" />
	</playerFish>

<!-- Fish -->

	<enemyFish name="Parrot A Small School" template="config/fish/ParrotASmallSchool.xml" poolSize="15">
		<spawnInfo playerSize="0"	maxCount="15"	respawnFreq="(0.2,0.5)" />
		<spawnInfo playerSize="14.5" maxCount="8"	respawnFreq="(0.5,1.0)" />
		<spawnInfo playerSize="17.5" maxCount="6"	respawnFreq="(0.5,1.0)" />
	</enemyFish>

	<enemyFish name="Barracuda Medium" template="config/fish/BarracudaMedium.xml" poolSize="6" >
		<spawnInfo playerSize="0"	maxCount="5"	respawnFreq="(0.5,1.0)" />
		<spawnInfo playerSize="14.5" maxCount="5"	respawnFreq="(1.0,1.5)" />
		<spawnInfo playerSize="17.5" maxCount="3"	respawnFreq="(1.0,2.0)" />
	</enemyFish>

	<enemyFish name="Tuna Large" template="config/fish/TunaLarge.xml" poolSize="2" >
		<spawnInfo playerSize="0"	maxCount="1"	respawnFreq="(3.0,7.0)" />
		<spawnInfo playerSize="14.5" maxCount="1"	respawnFreq="(2.0,4.0)" />
		<spawnInfo playerSize="17.5" maxCount="2"	respawnFreq="(1.0,3.0)" />
	</enemyFish> 

	<enemyFish name="Marlin Large" template="config/fish/MarlinLarge.xml" poolSize="2" displayMenuImage="true">
		<spawnInfo playerSize="0"	maxCount="1"	respawnFreq="(2.0,3.0)" />
		<spawnInfo playerSize="14.5" maxCount="1"	respawnFreq="(1.0,2.0)" />
		<spawnInfo playerSize="17.5" maxCount="2"	respawnFreq="(1.5,3.0)" />
	</enemyFish>

<!-- Hazards -->

	<enemyFish name="Jellies" template="config/fish/BasicJellyFish.xml" poolSize="3"  displayMenuImage="false" mode="uncounted">
		<spawnInfo playerSize="0"	maxCount="3"	respawnFreq="(4.0,8.0)" />
	</enemyFish>

<!-- Powerups -->

	<enemyFish name="Bubbles" template="config/fish/BonusBubbleStar.xml" poolSize="2"  displayMenuImage="false" mode="uncounted" >
		<spawnInfo playerSize="0"	maxCount="2"	respawnFreq="(5.0,7.0)" />
	</enemyFish> 
	
	<enemyFish name="Life Bubbles" template="config/fish/BonusBubbleFreeLife.xml" poolSize="1" mode="onlyUneaten" displayMenuImage="false">
		<spawnInfo playerSize="0"	maxCount="1"	respawnFreq="(30.0,45.0)" />
	</enemyFish>

	<enemyFish name="Shield Bubbles" template="config/fish/BonusBubbleShield.xml" poolSize="1"  displayMenuImage="false" mode="uncounted">
		<spawnInfo playerSize="0"	maxCount="1"	respawnFreq="(20.0,40.0)" />
	</enemyFish>

	<enemyFish name="GoldenMinnowMini" template="config/fish/GoldenMinnowMini.xml" poolSize="1" displayMenuImage="false" mode="onlyUneaten" >
		<spawnInfo playerSize="0"	maxCount="1"	respawnFreq="(15,40)" />
	</enemyFish>

	<music	introSound="stageIntro"
			outroSound="stageOutro"
			gameMusic="resources/music/track1.wav" />
		
</stage>
